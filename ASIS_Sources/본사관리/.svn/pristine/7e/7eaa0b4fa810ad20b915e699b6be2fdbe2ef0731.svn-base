Private Sub Data_Display()
    On Error GoTo ErrRtn

    Dim i           As Long
    
    If cboInput.ListIndex < 0 Then
        MsgBox "가맹점을 선택하세요.", vbInformation, "확인"
        Exit Sub
    End If
    
    '----------------------------------------------------------------
    ' SP_02001_00
    '----------------------------------------------------------------
    ReDim sValue(4)
    
    sValue(0) = Mid(cboInput.Text, 2, 6)
    sValue(0) = IIf(sValue(0) = "000000", "%", sValue(0))
    
    sValue(1) = Format(dtInput(0).Value, "YYYY-MM-DD")
    sValue(2) = Format(dtInput(1).Value, "YYYY-MM-DD")
    
    
    If HeadOffice = MASTER_OFFICE_CODE Then
        If DBOpen_Master(Mid(cboOffice.Text, 2, 4)) = False Then Exit Sub

        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecProMaster("SP_02014_01", sValue(), Err_Num, Err_Dec)
    Else
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecPro("SP_02014_01", sValue(), Err_Num, Err_Dec)
    End If
            
    With spdView(0)
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = 1:  .Text = RS01!가맹점코드 & ""         ' 1
            .Col = 2:  .Text = RS01!가맹점명 & ""       ' 2
            .Col = 3:  .Text = RS01!성명 & ""           ' 3
            .Col = 4:  .Text = RS01!전화번호 & ""       ' 4
            .Col = 5:  .Text = RS01!휴대전화 & ""       ' 5
            .Col = 6: .Text = RS01!접수일자 & " " & RS01!접수시간 & ""       '12
            
            .Col = 7:  .Text = Mid(RS01!판매취소일자, 12) & ""       ' 1
            
            .Col = 8:  .Text = Format(RS01!택번호 & "", "@@@-@@-@@@@")         ' 1
            .Col = 9:  .Text = RS01!접수번호 & ""       ' 2
            .Col = 10:  .Text = RS01!의류명 & ""         ' 7
            .Col = 11: .Text = RS01!금액 & ""           '11
            .Col = 12:  .Text = RS01!색상 & ""           ' 8
            .Col = 13:  .Text = RS01!내용 & ""           ' 9
            .Col = 14: .Text = RS01!상표 & ""           '10
            
            .Col = 15:  .Text = RS01!N접수시간 & ""       ' 1
            .Col = 16:  .Text = Format(RS01!N택번호 & "", "@@@-@@-@@@@")
            .Col = 17:  .Text = RS01!N접수번호 & ""       ' 2
            .Col = 18:  .Text = RS01!N의류명 & ""         ' 7
            .Col = 19:  .Text = RS01!N금액 & ""           '11
            .Col = 20:  .Text = RS01!N색상 & ""           ' 8
            .Col = 21:  .Text = RS01!N내용 & ""           ' 9
            .Col = 22:  .Text = RS01!N상표 & ""           '10
 
            
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
    End With
        
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub


