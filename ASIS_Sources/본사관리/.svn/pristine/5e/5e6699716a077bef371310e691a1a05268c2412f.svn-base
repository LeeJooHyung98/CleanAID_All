Private Sub Data_Display()
    On Error GoTo ErrRtn

    Dim i              As Long
    Dim iTotal(0 To 3) As Long

    ReDim sValue(9)
    
    '기본 입력 내용 확인
    If Trim(cboInput(1).Text) = "" And cboInput(1).ListCount > 1 Then cboInput(1).ListIndex = 1
    If Trim(cboInput(2).Text) = "" And cboInput(2).ListCount > 0 Then cboInput(2).ListIndex = cboInput(2).ListCount - 1
    If mskInput(0).ClipText = "" Then mskInput(0).Text = "0-000"
    If mskInput(1).ClipText = "" Then mskInput(1).Text = "9-999"
    
    
    sValue(0) = "0"
    sValue(1) = Format(dtInput(0).Value, "YYYY-MM-DD")    ' 일자
    sValue(2) = Format(dtInput(1).Value, "YYYY-MM-DD")    ' 일자
    sValue(3) = Mid(cboInput(0).Text, 2, 3) & "%"       ' 대리점명
    sValue(4) = Mid(cboInput(1).Text, 2, 3) & "%"       ' 품목명
    sValue(5) = Mid(cboInput(2).Text, 2, 3) & "%"       ' 품목명
    sValue(6) = mskInput(0).ClipText & "%"              ' 택번호
    sValue(7) = mskInput(1).ClipText & "%"              ' 택번호
    sValue(9) = txtInput(5).Text & "%"                  ' 고객명
    
    If chkSelect(0).Value = False And chkSelect(1).Value = False And chkSelect(2).Value = False Then
        sValue(8) = "0"
    ElseIf chkSelect(0).Value = True And chkSelect(1).Value = False And chkSelect(2).Value = False Then
        sValue(8) = "1"
    ElseIf chkSelect(0).Value = False And chkSelect(1).Value = True And chkSelect(2).Value = False Then
        sValue(8) = "2"
    ElseIf chkSelect(0).Value = False And chkSelect(1).Value = False And chkSelect(2).Value = True Then
        sValue(8) = "3"
    ElseIf chkSelect(0).Value = True And chkSelect(1).Value = True And chkSelect(2).Value = False Then
        sValue(8) = "4"
    ElseIf chkSelect(0).Value = True And chkSelect(1).Value = False And chkSelect(2).Value = True Then
        sValue(8) = "5"
    ElseIf chkSelect(0).Value = False And chkSelect(1).Value = True And chkSelect(2).Value = True Then
        sValue(8) = "6"
    ElseIf chkSelect(0).Value = True And chkSelect(1).Value = True And chkSelect(2).Value = True Then
        sValue(8) = "7"
    End If
    
    Set RS01 = New ADODB.Recordset
    Set RS01 = ExecPro("SP_02004_00", sValue(), Err_Num, Err_Dec)
    
    spdView.MaxCols = RS01.Fields.Count
    spdView.MaxRows = RS01.RecordCount
    
    'Call spdDisplay(RS01)
    Call fpSpread_Display(spdView, RS01)
    Call GetColWidth(REG_App, Me.Name, spdView)
    
    For i = 1 To spdView.MaxRows
        spdView.Row = i
        spdView.Col = 6
        iTotal(0) = iTotal(0) + Val(spdView.Value)
        
        spdView.Col = 7
        If Trim(spdView.Text) = "반" Then
            iTotal(1) = iTotal(1) + 1
        ElseIf Trim(spdView.Text) = "드재" Then
            iTotal(2) = iTotal(2) + 1
        ElseIf Trim(spdView.Text) = "드수" Or Trim(spdView.Text) = "수" Then
            iTotal(3) = iTotal(3) + 1
        End If
    Next i
    
    txtNum(0).Text = Format(spdView.MaxRows, "#,##0")
    txtNum(1).Text = Format(iTotal(0), "#,##0")
    txtNum(2).Text = Format(iTotal(1), "#,##0")
    txtNum(3).Text = Format(iTotal(2), "#,##0")
    txtNum(4).Text = Format(iTotal(3), "#,##0")
        
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

