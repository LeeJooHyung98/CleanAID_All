Private Sub Data_Display()
    On Error GoTo ErrRtn

    Dim i As Integer
    
    If Trim(cboOffice.Text) = "" Then
        MsgBox "지사를 선택하세요.", vbInformation, "확인"
        
        cboOffice.SetFocus
        
        Exit Sub
    End If
    
    spdView.MaxRows = 0
    
    For i = 0 To 9
        txtNum(i).Value = 0
    Next i
    
    ReDim sValue(2)
    
    sValue(0) = Mid(cboOffice.Text, 2, 4)
    sValue(1) = Format(dtInput.Value, "YYYY-MM-DD")
    
    Select Case True
        Case optSelect(0).Value: sValue(2) = ""   '전체
        Case optSelect(1).Value: sValue(2) = "2"  '월수금
        Case optSelect(2).Value: sValue(2) = "1"  '일수금
    End Select
    
    If HeadOffice = MASTER_OFFICE_CODE Then
        If DBOpen_Master(Mid(cboOffice.Text, 2, 4)) = False Then Exit Sub
        
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecProMaster("SP_04001_02", sValue(), Err_Num, Err_Dec)
    Else
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecPro("SP_04001_02", sValue(), Err_Num, Err_Dec)
    End If
        
    With spdView
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = 1: .Text = RS01!가맹점코드 & ""                   ' 1
            .Col = 2: .Text = RS01!가맹점명 & ""                     ' 2
            
            If RS01!수금구분 = "1" Then
                .Col = 3: .Text = ""
            Else
                .Col = 3: .Text = "월"
            End If
            
            .Col = 4: .Text = RS01!접수금액 & ""                     ' 3
            .Col = 5: .Text = RS01!접수수량 & ""                     ' 4
            .Col = 6: .Text = RS01!출고수량 & ""                     ' 5
            
            If RS01!접수수량 = 0 Then
                .Col = 7: .Text = 0 & ""                             ' 6
            Else
                .Col = 7: .Text = RS01!접수금액 / RS01!접수수량 & "" ' 6
            End If
            
            .Col = 8:  .Text = RS01!재세탁수량 & ""                   ' 7
            .Col = 9:  .Text = RS01!수선수량 & ""                     ' 8
            .Col = 10: .Text = RS01!반품환불건수 & ""                 ' 9
                        
            If Len(RS01!시작택번호) = 9 Then
                .Col = 11: .Text = Format(Right(RS01!시작택번호, 6), "00-0000") & "" '10
            Else
                .Col = 11: .Text = RS01!시작택번호 & ""                        '10
            End If
            
            If Len(RS01!종료택번호) = 9 Then
                .Col = 12: .Text = Format(Right(RS01!종료택번호, 6), "00-0000") & "" '11
            Else
                .Col = 12: .Text = RS01!종료택번호 & ""                        '11
            End If
            
            .Col = 13: .Text = "1"                                             '12
            
            txtNum(0).Value = txtNum(0).Value + RS01!접수금액
            
            If RS01!수금구분 = "1" Then
                txtNum(3).Value = txtNum(3).Value + RS01!접수금액
            Else
                txtNum(4).Value = txtNum(4).Value + RS01!접수금액
            End If
            
            txtNum(5).Value = txtNum(5).Value + RS01!접수수량
            txtNum(6).Value = txtNum(6).Value + RS01!출고수량
            txtNum(7).Value = txtNum(7).Value + RS01!재세탁수량
            txtNum(8).Value = txtNum(8).Value + RS01!수선수량
            txtNum(9).Value = txtNum(9).Value + RS01!반품환불건수
            
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
        
        '-------------------------------------------
        ' 접수금액 순으로 역순 정렬
        '-------------------------------------------
        .SortKey(1) = 4
        .SortKeyOrder(1) = SortKeyOrderDescending
        .Sort -1, -1, -1, -1, SortByRow
        '-------------------------------------------
        
        If txtNum(5).Value > 0 Then
            txtNum(1).Value = txtNum(0).Value / txtNum(5).Value
        Else
            txtNum(1).Value = 0
        End If
        
        .Redraw = True
    End With
        
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

