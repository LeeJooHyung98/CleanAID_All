Private Sub DataPrint()
    On Error GoTo ErrRtn
    
    Dim XML         As String
    Dim i           As Integer
    Dim 수금구분    As String
    Dim FileNumber
        
    If spdView.MaxRows = 0 Then Exit Sub
    
    Select Case True
        Case optSelect(0).Value: 수금구분 = optSelect(0).Caption & ""
        Case optSelect(1).Value: 수금구분 = optSelect(1).Caption & ""
        Case optSelect(2).Value: 수금구분 = optSelect(2).Caption & ""
    End Select
    
    FileNumber = FreeFile
    
    Open App.Path & "\XML\일일수금등록.XML" For Output As #FileNumber
    
    Print #FileNumber, "<?xml version=""1.0"" encoding=""EUC-KR""?>"
    Print #FileNumber, "<root>"
    
          XML = "    <조건>"
    XML = XML & "        <지사명>지 사 명 : " & Func_Replace(cboOffice.Text) & "</지사명>"
    XML = XML & "        <수금구분>수금구분 : " & 수금구분 & "</수금구분>"
    XML = XML & "        <수금일자>수금일자 : " & Format(dtInput.Value, "YYYY년 MM월 DD일") & "</수금일자>"
    XML = XML & "        <수금액>" & txtNum(0).Text & "</수금액>"
    XML = XML & "        <단가>" & txtNum(1).Text & "</단가>"
    XML = XML & "        <카드>" & txtNum(2).Text & "</카드>"
    XML = XML & "        <일수금>" & txtNum(3).Text & "</일수금>"
    XML = XML & "        <월수금>" & txtNum(4).Text & "</월수금>"
    XML = XML & "        <접수량>" & txtNum(5).Text & "</접수량>"
    XML = XML & "        <출고량>" & txtNum(6).Text & "</출고량>"
    XML = XML & "        <재세탁>" & txtNum(7).Text & "</재세탁>"
    XML = XML & "        <수선>" & txtNum(8).Text & "</수선>"
    XML = XML & "        <반품>" & txtNum(9).Text & "</반품>"
    XML = XML & "   </조건>"
    Print #FileNumber, XML
    
    With spdView
        For i = 1 To .MaxRows
            .Row = i
            .Col = 13
            
            If .Text = "1" Then
                                 XML = "    <Data>"
                           XML = XML & "        <순위>" & i & "</순위>"
                .Col = 1:  XML = XML & "        <가맹점코드>" & .Text & "</가맹점코드>"
                .Col = 2:  XML = XML & "        <가맹점명>" & Func_Replace(.Text) & "</가맹점명>"
                .Col = 3:  XML = XML & "        <구분>" & Func_Replace(.Text) & "</구분>"
                .Col = 4:  XML = XML & "        <수금액>" & .Text & "</수금액>"
                .Col = 5:  XML = XML & "        <접수량>" & .Text & "</접수량>"
                .Col = 6:  XML = XML & "        <출고량>" & .Text & "</출고량>"
                .Col = 7:  XML = XML & "        <단가>" & .Text & "</단가>"
                .Col = 8:  XML = XML & "        <재세탁>" & .Text & "</재세탁>"
                .Col = 9:  XML = XML & "        <수선>" & .Text & "</수선>"
                .Col = 10: XML = XML & "        <반품>" & .Text & "</반품>"
                .Col = 11: XML = XML & "        <시작택번호>" & .Text & "</시작택번호>"
                .Col = 12: XML = XML & "        <종료택번호>" & .Text & "</종료택번호>"
                           XML = XML & "   </Data>"
                Print #FileNumber, XML
            End If
        
        Next i
    
        Print #FileNumber, "</root>"
        Close #FileNumber
    End With
    
    With rpt일일수금대장
        .dc.FileURL = App.Path & "\XML\일일수금등록.XML"
        .PrintReport False
        
        '.Show 1
    End With

    Unload rpt일일수금대장
    
    Exit Sub

ErrRtn:
    MsgBox Err.Description, vbInformation, "오류"
    Screen.MousePointer = 0
End Sub

