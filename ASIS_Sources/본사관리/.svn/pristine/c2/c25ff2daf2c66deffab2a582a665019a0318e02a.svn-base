Private Sub Form_Activate()
    cmdBtn(0).Enabled = True
    cmdBtn(5).Enabled = True
    cmdBtn(6).Enabled = True
    
    '본사 여부
    If HeadOffice = MASTER_OFFICE_CODE Then
        cboOffice.Locked = False
    Else
        cboOffice.Locked = True
    End If
    
    pnlHeader.Caption = " " & Me.Caption & " (" & Me.Name & ")"
End Sub

Private Sub Form_Load()
    On Error GoTo ErrRtn
    
    With spdView
        .MaxRows = 0
        .RowHeight(-1) = 14
        
        'Spread 8 - 디자인
        .HighlightHeaders = HighlightHeadersOff
        .AppearanceStyle = AppearanceStyleEnhanced
        .ScrollBarStyle = ScrollBarStyleVisualStyle
        
        '선택된 Row
        .SelBackColor = &HFFFFC0 '황색 ^^
        .SelForeColor = &H0&     '검은글씨
'        .OperationMode = OperationModeSingle
        
        'Init the User Sort
        .UserColAction = UserColActionSort
    End With
    
    dtInput(0).Value = Format(Date, "yyyy-mm")
    dtInput(1).Value = Format(Date, "yyyy-mm")
    
    Call Get_지사리스트(cboOffice)
    
    Dim i As Integer
    
    With cboOffice
        For i = 0 To .ListCount - 1
            If Mid(.List(i), 2, 4) = HeadOffice Then
                .ListIndex = i
                
                Exit For
            End If
        Next i
    End With
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub Form_Unload(Cancel As Integer)
    P_04011_Flag = False
End Sub

Private Sub Data_Display()
    On Error GoTo ErrRtn
    
    Dim i As Integer
    Dim nCnt    As Long
    Dim dblTotal As Double
    
        
    ReDim sValue(3)
    
    sValue(0) = Mid(cboOffice.Text, 2, 4)
    sValue(1) = Mid(cboInput.Text, 2, 6)
    sValue(2) = Format(dtInput(0).Value, "YYMMDD")
    sValue(3) = Format(dtInput(1).Value, "YYMMDD")
    
    If HeadOffice = MASTER_OFFICE_CODE Then
        If DBOpen_Master(Mid(cboOffice.Text, 2, 4)) = False Then Exit Sub
        
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecProMaster("SP_04020_1_00", sValue(), Err_Num, Err_Dec)
    Else
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecPro("SP_04020_1_00", sValue(), Err_Num, Err_Dec)
    End If
        
    With spdView
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            
            .Col = 1:  .Text = RS01!가맹점코드 & ""               '
            .Col = 2:  .Text = RS01!가맹점명 & ""           '
            
            If Left(RS01!메시지2, 2) = "OK" Then
                .Col = 3: .Text = "승인": .ForeColor = vbBlue
                
                nCnt = nCnt + 1
                dblTotal = dblTotal + Val(RS01!결제금액 & "")
                
            Else
                .Col = 3: .Text = "취소": .ForeColor = vbRed
            End If
            
            .Col = 4:  .Text = RS01!성명 & ""               '
            .Col = 5:  .Text = RS01!전화번호 & ""           '
            .Col = 6:  .Text = RS01!휴대전화 & ""           '
            .Col = 7:  .Text = RS01!접수번호 & ""           '
            .Col = 8:  .Text = RS01!승인번호 & ""           '
            .Col = 9:  .Text = RS01!승인일자 & ""           '
            .Col = 10: .Text = RS01!승인시간 & ""           '
            .Col = 11: .Text = RS01!입력방법 & ""           '
            .Col = 12: .Text = RS01!결제금액 & ""           '
            .Col = 13: .Text = RS01!사용자정보 & "" '
            .Col = 14: .Text = RS01!메시지1 & ""            '
            .Col = 15: .Text = RS01!메시지2 & ""            '
            .Col = 16: .Text = RS01!고객코드 & ""           '
            
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
        
        .Redraw = True
        
        txtNum(0).Text = nCnt
        txtNum(1).Text = dblTotal
        
    End With
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

