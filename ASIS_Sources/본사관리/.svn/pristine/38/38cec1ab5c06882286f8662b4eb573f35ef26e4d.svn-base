Private Sub Data_Display()
On Error GoTo ErrRtn
    
    Dim nCnt    As Long
    Dim vTemp   As Variant
    Dim Query   As String
    
    Query = "SELECT evt_id, title, case shop_type WHEN 'SHOP' THEN '일반' WHEN 'EMART' THEN '이마트' WHEN 'ALL' THEN '전체' END as SHOP_TYPE FROM BIZ_PURIO..tbl_event"
    Set RS01 = New ADODB.Recordset
    Set RS01 = ExecQuery(Query, Err_Num, Err_Dec)
    
    With spdView
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = 1: .Text = RS01!evt_id & "" ' 1
            .Col = 2: .Text = RS01!Title & ""   ' 2
            .Col = 3: .Text = RS01!shop_type & ""   ' 2
            
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
        
        .Redraw = True
    End With
    
    Data_Clear
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub Data_Display2(evt_id As String)
On Error GoTo ErrRtn
    
    Dim nCnt    As Long
    Dim vTemp   As Variant
    Dim Query   As String
    
    Query = "SELECT * FROM BIZ_PURIO..tbl_event WHERE evt_id = '" & evt_id & "'"
    Set RS01 = New ADODB.Recordset
    Set RS01 = ExecQuery(Query, Err_Num, Err_Dec)
    txtTitle.Tag = evt_id
    txtTitle.Text = RS01!Title
    dtInput(0).Value = RS01!start_date
    dtInput(1).Value = RS01!end_date
    Select Case RS01!shop_type
    Case "ALL"
        cboGubun.Text = "전체"
    Case "SHOP"
        cboGubun.Text = "일반가맹점"
    Case "EMART"
        cboGubun.Text = "이마트"
    End Select
    txtEventMsg.Text = RS01!MSG
    txtReMsg.Text = RS01!re_msg

    If Mid(RS01!Week, 1, 1) = "1" Then chkDay(0).Value = xtpChecked Else chkDay(0).Value = xtpUnchecked
    If Mid(RS01!Week, 2, 1) = "1" Then chkDay(1).Value = xtpChecked Else chkDay(1).Value = xtpUnchecked
    If Mid(RS01!Week, 3, 1) = "1" Then chkDay(2).Value = xtpChecked Else chkDay(2).Value = xtpUnchecked
    If Mid(RS01!Week, 4, 1) = "1" Then chkDay(3).Value = xtpChecked Else chkDay(3).Value = xtpUnchecked
    If Mid(RS01!Week, 5, 1) = "1" Then chkDay(4).Value = xtpChecked Else chkDay(4).Value = xtpUnchecked
    If Mid(RS01!Week, 6, 1) = "1" Then chkDay(5).Value = xtpChecked Else chkDay(5).Value = xtpUnchecked
    If Mid(RS01!Week, 7, 1) = "1" Then chkDay(6).Value = xtpChecked Else chkDay(6).Value = xtpUnchecked
    RS01.Close
    Set RS01 = Nothing
    
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub Data_Clear()
On Error GoTo ErrRtn
    
   
    txtTitle.Tag = ""
    txtTitle.Text = ""
    dtInput(0).Value = Now
    dtInput(1).Value = Now
    
    cboGubun.Text = "전체"
    txtEventMsg.Text = ""
    txtReMsg.Text = ""

    chkDay(0).Value = xtpUnchecked
    chkDay(1).Value = xtpUnchecked
    chkDay(2).Value = xtpUnchecked
    chkDay(3).Value = xtpUnchecked
    chkDay(4).Value = xtpUnchecked
    chkDay(5).Value = xtpUnchecked
    chkDay(6).Value = xtpUnchecked
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub Data_Save()
On Error GoTo ErrRtn
    
    Dim Week As String
    Dim Gubun As String
    Week = chkDay(0).Value & chkDay(1).Value & chkDay(2).Value & chkDay(3).Value & chkDay(4).Value & chkDay(5).Value & chkDay(6).Value
    
    Select Case cboGubun.Text
    Case "전체"
        Gubun = "ALL"
    Case "일반가맹점"
        Gubun = "SHOP"
    Case "이마트"
        Gubun = "EMART"
    End Select
    
    Dim Query As String
    If txtTitle.Tag <> "" Then
        Query = ""
        Query = Query & " UPDATE BIZ_PURIO..tbl_event"
        Query = Query & " SET "
        Query = Query & "   TITLE      ='" & txtTitle.Text & "',"
        Query = Query & "   MSG        ='" & txtEventMsg.Text & "',"
        Query = Query & "   RE_MSG     ='" & txtReMsg.Text & "',"
        Query = Query & "   WEEK       ='" & Week & "',"
        Query = Query & "   START_DATE ='" & dtInput(0).Value & "',"
        Query = Query & "   END_DATE   ='" & dtInput(1).Value & "',"
        Query = Query & "   SHOP_TYPE  ='" & Gubun & "'"
        Query = Query & " WHERE"
        Query = Query & "   EVT_ID = '" & txtTitle.Tag & "'"
    Else
        Query = ""
        Query = "INSERT INTO BIZ_PURIO..tbl_event(TITLE, MSG, RE_MSG, WEEK, START_DATE, END_DATE, SHOP_TYPE) "
        Query = Query & "VALUES ("
        Query = Query & "'" & txtTitle.Text & "',"
        Query = Query & "'" & txtEventMsg.Text & "',"
        Query = Query & "'" & txtReMsg.Text & "',"
        Query = Query & "'" & Week & "',"
        Query = Query & "'" & dtInput(0).Value & "',"
        Query = Query & "'" & dtInput(1).Value & "',"
        Query = Query & "'" & Gubun & "'"
        Query = Query & ")"
    End If
    
    Call ExecQuery(Query, Err_Num, Err_Dec)
    Data_Display
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub
Private Sub Form_Load()
    Data_Display
    dtInput(0).Value = Now
    dtInput(1).Value = Now
End Sub

Private Sub spdView_Click(ByVal Col As Long, ByVal Row As Long)
    With spdView
        .Row = Row: .Col = 1
        Data_Display2 (.Text)
    End With
End Sub
