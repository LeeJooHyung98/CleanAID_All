Private Sub DataSave()
    On Error GoTo ErrRtn
    
    Dim vText   As Variant
    Dim nRow    As Long
    
    ReDim sValue(1)
    
    For nRow = 1 To spdView(1).DataRowCnt
    
        spdView(1).GetText 1, nRow, vText:         sValue(0) = CStr(vText)
        spdView(1).GetText 4, nRow, vText:         sValue(1) = Replace(CStr(vText), "-", "")
        
        
        Call ExecPro("SP_05015_03", sValue(), Err_Num, Err_Dec)
    
    Next nRow
    
    MsgBox "저장 완료", vbInformation, "확인"
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)

End Sub
