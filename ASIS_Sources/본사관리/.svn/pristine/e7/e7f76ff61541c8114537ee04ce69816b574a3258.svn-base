        If IsNull(RS01!프린터) Then
            cboInput(5).ListIndex = -1                                   '15
        Else
            cboInput(5).ListIndex = -1
            
            For i = 0 To cboInput(5).ListCount - 1
                If RS01!프린터 = Mid(cboInput(5).List(i), 2, 1) Then
                    cboInput(5).ListIndex = i                            '15
                    Exit For
                End If
            Next i
