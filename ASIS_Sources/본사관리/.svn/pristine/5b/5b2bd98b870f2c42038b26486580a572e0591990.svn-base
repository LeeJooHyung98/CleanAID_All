Dim sValue() As String

Dim Err_Num As Long
Dim Err_Dec As String

Private Sub cboOffice_Click()
    Call Data_Display
End Sub

Private Sub cmdBtn_Click(Index As Integer)
    On Error GoTo ErrRtn
    
    Select Case Index
        Case 0: Call Data_Display   ' 조회
'        Case 1: Call DataAdd        ' 신규
'        Case 2: Call DataSave       ' 저장
'        Case 3: Call DataDelete     ' 삭제
'        Case 4: Call DataCancel     ' 취소
        Case 5: 'Call DataPrint      ' 인쇄
'        Case 6: Call Export_Excel(P_00000.cdgExcel, spdView)      ' 엑셀
        Case 7: Unload Me           ' 종료
    End Select
    
'    Me.MousePointer = 0
    
    Exit Sub
    
ErrRtn:
    Me.MousePointer = 0
    
    If Err.Number = "0" Then
        
    ElseIf Err.Number = "91" Then
        End
    Else
        Resume Next
    End If
End Sub

Private Sub Form_Activate()
    cmdBtn(0).Enabled = True
    cmdBtn(1).Enabled = False
    cmdBtn(2).Enabled = False
    cmdBtn(3).Enabled = False
    cmdBtn(4).Enabled = False
    
    pnlHeader.Caption = " " & Me.Caption & " (" & Me.Name & ")"

End Sub

'Private Sub spdDisplay(RS As ADODB.Recordset)
'    Call fpSpread_Display(spdView, RS)
'End Sub

Private Sub Form_Load()
    On Error GoTo ErrRtn
    
    With spdView
        .MaxRows = 0
        .RowHeight(-1) = 14
        
        'Spread 8 - 디자인
        .HighlightHeaders = HighlightHeadersOff
        .AppearanceStyle = AppearanceStyleEnhanced
        .ScrollBarStyle = ScrollBarStyleVisualStyle
        
        '선택된 Row
        .SelBackColor = &HFFFFC0 '황색 ^^
        .SelForeColor = &H0&     '검은글씨
        .OperationMode = OperationModeNormal
        
        'Init the User Sort
        .UserColAction = UserColActionSort
    End With


    Call Get_지사리스트(cboOffice)
        
    Dim i As Integer
    
    With cboOffice
        For i = 0 To .ListCount - 1
            If Mid(.List(i), 2, 4) = HeadOffice Then
                .ListIndex = i
                
                Exit For
            End If
        Next i
    End With
        
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub Form_Unload(Cancel As Integer)
    P_01002_Flag = False
End Sub






Private Sub spdView_LeaveCell(ByVal Col As Long, ByVal Row As Long, ByVal NewCol As Long, ByVal NewRow As Long, Cancel As Boolean)
    If NewRow <> -1 Then
        spdView.Row = Row
        spdView.Col = -1
        spdView.BackColor = vbWhite

        spdView.Row = NewRow
        spdView.Col = -1
        spdView.BackColor = glbYellow
    End If
End Sub



Private Sub Data_Display()
    On Error GoTo ErrRtn
    
    Dim nCnt(3)    As Long
    Dim nTime   As Integer
    
        
    ReDim sValue(0)
    
    sValue(0) = Mid(cboOffice.Text, 2, 4)
    sValue(0) = IIf(sValue(0) = "0000", "%", sValue(0))
    
    Set RS01 = New ADODB.Recordset
    Set RS01 = ExecPro("SP_04030_00", sValue(), Err_Num, Err_Dec)
    
    With spdView
        .MaxRows = 0
        .Redraw = False
        
        nCnt(0) = 0:   nCnt(1) = 0:   nCnt(2) = 0:   nCnt(3) = 0
        nTime = IIf(IsNumeric(SSPanel7(18).Tag) = True, Val(SSPanel7(18).Tag), 10)

        
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
             nCnt(0) = nCnt(0) + 1
            .Col = 1:  .Text = RS01!가맹점코드 & ""               '
            .Col = 2:  .Text = RS01!가맹점명 & ""           '
            .Col = 3:  .Text = RS01!가맹점상태 & ""
            If RS01!가맹점상태 & "" = "폐점준비중" Then
                nCnt(2) = nCnt(2) + 1
                .Col = -1:  .BackColor = vbGreen
            End If
            
            '
            .Col = 4:  .Text = RS01!지사코드 & ""               '
            .Col = 5:  .Text = RS01!매장전화번호 & ""               '
            .Col = 6:  .Text = RS01!당일전송정보 & ""           '
            .Col = 7:  .Text = RS01!최종전송정보 & ""            '
                        
            If IsDate(RS01!당일전송정보) = True Then nCnt(1) = nCnt(1) + 1
            If IsDate(RS01!당일전송정보) = True And IsDate(RS01!서버시간) = True Then
                If DateDiff("n", RS01!당일전송정보 & "", RS01!서버시간 & "") > nTime Then
                    .Col = -1:  .BackColor = vbRed
                End If
            End If
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
        
        .Redraw = True
        
        txtNum(0).Text = Format(nCnt(0), "#,##0")
        txtNum(1).Text = Format(nCnt(1), "#,##0")
        txtNum(2).Text = Format(nCnt(2), "#,##0")
        txtNum(3).Text = Format(nCnt(0) - nCnt(1), "#,##0")
        
    
    End With
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub SSPanel7_DblClick(Index As Integer)
    Dim sTime   As String
    If Index = 18 Then
        sTime = InputBox("확인할 분을 입력하십시요. 최소 10~ 입력 가능 합니다.")
        If IsNumeric(sTime) = True Then SSPanel7(Index).Tag = sTime
    End If

End Sub
