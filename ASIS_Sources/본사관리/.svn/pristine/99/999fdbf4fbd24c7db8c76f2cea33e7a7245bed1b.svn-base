Attribute VB_Name = "P_03015"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Dim RS01 As ADODB.Recordset
Dim sValue() As String
Dim sSpValue() As String

Dim Err_Num As Long
Dim Err_Dec As String

Dim 지사출고번호 As Integer

Private Sub SPR_Resize()
    On Error GoTo ErrRtn
    
    cmdBtn(8).Left = Me.Width - cmdBtn(8).Width - 200

    Exit Sub
    
ErrRtn:

End Sub

Private Sub cboOffice_Click()
    If cboOffice.ListIndex < 0 Then Exit Sub
    
    Call Data_Display
End Sub

'-----------------------------------------------------------------
'
'-----------------------------------------------------------------
Private Sub Data_Display()
    On Error GoTo ErrRtn

    Dim i As Integer
    
    ReDim sValue(2)
    
    spdView1.MaxRows = 0
    
    For i = 1 To 8
        txtNum(i).Value = 0
    Next i
    
    txtNum(0).Value = 0
    
    sValue(0) = Mid(cboOffice.Text, 2, 4)
    sValue(1) = Format(dtInput(0).Value, "YYYY-MM-DD")
    sValue(2) = Format(dtInput(1).Value, "YYYY-MM-DD")
    
    If HeadOffice = MASTER_OFFICE_CODE Then
        If DBOpen_Master(Mid(cboOffice.Text, 2, 4)) = False Then Exit Sub

        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecProMaster("SP_03015_00", sValue(), Err_Num, Err_Dec)
    Else
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecPro("SP_03015_00", sValue(), Err_Num, Err_Dec)
    End If
        
    With spdView
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = 1: .Text = "1"
            .Col = 2: .Text = RS01!가맹점코드 & ""
            .Col = 3: .Text = RS01!가맹점명 & ""
            .Col = 4: .Text = RS01!출고수량 & ""
            
            txtNum(0).Value = txtNum(0).Value + RS01!출고수량
            
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
    
        .Redraw = True
    End With
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub cboOffice_KeyPress(KeyAscii As Integer)
    If KeyAscii = vbKeyReturn Then
        SearchString_한글 KeyAscii
'    Else
       ' SearchString KeyAscii
    End If

End Sub

Private Sub cmdBtn_Click(Index As Integer)
    Dim iRow        As Integer
    Dim 가맹점코드  As String
    Dim Query       As String

    On Error GoTo ErrRtn
    
    ' 종료 버튼을 비활성화 한다.
    P_00000.CommandBars.Item(2).Controls(7).Enabled = False
    
    cmdBtn(Index).Enabled = False
    DoEvents
    
    Select Case Index
        Case 0:
            Call Data_Display    ' 조회
            
            cmdBtn(Index).Enabled = True
            
        Case 1: 'Call DataAdd        ' 신규
        Case 2: 'Call DataSave       ' 저장
        Case 3: 'Call DataDelete     ' 삭제
        Case 4: 'Call DataCancel     ' 취소
        Case 5: 'Call DataPrint      ' 인쇄
        Case 6: Call Export_Excel(P_00000.cdgExcel, spdView1)      ' 엑셀
        Case 7: Unload Me            ' 종료
        Case 8: Data_Action
        
            
    End Select
    ' 종료 버튼을 활성/비활성화 한다.
    P_00000.CommandBars.Item(2).Controls(7).Enabled = True
    
    Exit Sub
    
ErrRtn:
    Me.MousePointer = 0
    cmdBtn(Index).Enabled = True
    cmdBtn(7).Enabled = True
    spdView.Enabled = True
    
    '--------------------------------------------------------------------------------------------------------
    ' 작업 시작 로그를 저장한다.
    sSpValue(0) = Store.Code: sSpValue(1) = dtInput(2).Value:   sSpValue(2) = 지사출고번호
    sSpValue(3) = 가맹점코드: sSpValue(5) = Now:                sSpValue(7) = "오류"
    ' sValue(4) = Now  <- 위쪽에서 기록
    Call P_03015_Data(sSpValue)
    '--------------------------------------------------------------------------------------------------------

    ' 종료 버튼을 활성/비활성화 한다.
    P_00000.CommandBars.Item(2).Controls(7).Enabled = True
    
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

Private Sub Data_Update_OLD()
    Dim i            As Long
    
    Dim 가맹점코드   As String
    Dim 택번호       As String
    Dim 출고일자     As String
    
    Dim 지사출고물품 As String
    Dim 지사출고상태 As String
    
    Dim Query        As String
        
    If spdView.ActiveRow <= 0 Then Exit Sub
    
    ProgressBar.Value = 0
    ProgressBar.Min = 0
    ProgressBar.Max = 100
    ProgressBar.Visible = True
    
    spdView.Row = spdView.ActiveRow
    spdView.Col = 2: 가맹점코드 = spdView.Text & ""
    
    With spdView1
        출고일자 = Format(dtInput(2).Value, "YYYY-MM-DD") & " " & Format(Now, "hh:mm:ss")
        
        For i = 1 To .MaxRows
            ProgressBar.Value = (i / spdView1.MaxRows) * 100
            DoEvents
            
            .Row = i
            
            '-------------------------------------------------------------------------------------
            ' 1) TB_입출고에 저장하기
            '-------------------------------------------------------------------------------------
            .Col = 1: 택번호 = Replace(.Text, "-", "") & "" '
            .Col = 4: 지사출고물품 = .Text & ""             '
            .Col = 6: 지사출고상태 = .Text & ""             '
            
            Query = "UPDATE TB_입출고 SET 지사출고일자 = '" & 출고일자 & "'"
            Query = Query & "           , 지사출고물품 = '" & 지사출고물품 & "'"
            Query = Query & "           , 지사출고상태 = '" & 지사출고상태 & "'"
            Query = Query & "           , 지사출고번호 =  " & 지사출고번호
            Query = Query & " WHERE 가맹점코드 = '" & 가맹점코드 & "'"
            Query = Query & "   AND 택번호     = '" & 택번호 & "'"
            Query = Query & "   AND (RTRIM(지사출고일자) = '' OR 지사출고일자 IS NULL)"
            ADOCon.Execute Query
            
            
            '-------------------------------------------------------------------------------------
            ' 2) SCANOUTPUT_LOG_TB에 저장하기
            '-------------------------------------------------------------------------------------
            ReDim sValue(9)
            
            .Col = 8:  sValue(0) = Trim(.Text) & ""             ' 0 SCAN_DATE
            .Col = 9:  sValue(1) = .Text & ""                   ' 1 PDA_NO
                       sValue(2) = 가맹점코드                   ' 2 가맹점코드
                       sValue(3) = 택번호 & ""                  ' 3 TAG_NO
            .Col = 4:  sValue(4) = Trim(.Text) & ""             ' 4 SCAN_FLAG1
            .Col = 6:  sValue(5) = Trim(.Text) & ""             ' 5 SCAN_FLAG2
            .Col = 10: sValue(6) = Trim(.Text) & ""             ' 6 OUT_FLAG
                       sValue(7) = 출고일자                     ' 7 지사출고일자
                       sValue(8) = 지사출고번호                 ' 8 OUT_COUNT
                       sValue(9) = ""                           ' 9 수기 일일출고에서 사용하는 필드
                       
                       
            Call ExecPro("SP_03015_02", sValue(), Err_Num, Err_Dec)
            
            .Col = 10
            If .Text = "N" Then
                '-------------------------------------------------------------------------------------
                ' 2) TB_지사출고오류 저장하기
                '-------------------------------------------------------------------------------------
                ReDim sValue(10)
                
                           sValue(0) = 가맹점코드                   ' 0 가맹점코드
                           sValue(1) = 택번호 & ""                  ' 1 TAG_NO
                           sValue(2) = 출고일자                     ' 2 지사출고일자
                .Col = 4:  sValue(3) = Trim(.Text) & ""             ' 3 지사출고물품
                .Col = 6:  sValue(4) = Trim(.Text) & ""             ' 4 지사출고상태
                           sValue(5) = 지사출고번호                 ' 5 지사출고번호
                .Col = 8:  sValue(6) = Trim(.Text) & ""             ' 6 SCAN_DATE
                .Col = 9:  sValue(7) = .Text & ""                   ' 7 PDA_NO
                .Col = 10: sValue(8) = .Text & ""                   ' 8 입고여부
                           sValue(9) = "N"                          ' 9 오류처리여부
                           sValue(10) = ""                          '10 오류처리일자
                
                Call ExecPro("SP_03015_03", sValue(), Err_Num, Err_Dec)
            End If
        Next i
        
        '-------------------------------------------------------------------------------------
        ' 3) SCANOUTPUT_TB 삭제하기
        '-------------------------------------------------------------------------------------
        Query = "DELETE FROM SCANOUTPUT_TB"
        Query = Query & " WHERE STORE_CD = '" & 가맹점코드 & "'"
        Query = Query & "   AND SUBSTRING(SCAN_DATE,1,10) >= '" & Format(dtInput(0).Value, "YYYY-MM-DD") & "'"
        Query = Query & "   AND SUBSTRING(SCAN_DATE,1,10) <= '" & Format(dtInput(1).Value, "YYYY-MM-DD") & "'"
        ADOCon.Execute Query
        
        spdView1.MaxRows = 0
    End With
    
    ProgressBar.Visible = False
End Sub


Private Sub Data_Update()
    Dim i            As Long
    
    Dim 가맹점코드   As String
    Dim 택번호       As String
    Dim 출고일자     As String
    
    Dim 지사출고물품 As String
    Dim 지사출고상태 As String
    
    Dim Query        As String
        
    If spdView.ActiveRow <= 0 Then Exit Sub
    
    ProgressBar.Value = 0
    ProgressBar.Min = 0
    ProgressBar.Max = 100
    ProgressBar.Visible = True
    
    DoEvents
    
    spdView.Row = spdView.ActiveRow
    spdView.Col = 2: 가맹점코드 = spdView.Text & ""
    
    With spdView1
        ' 처리시간이 길어저도 동일한 처리 시간을 설정 하기 위하여
        출고일자 = Format(dtInput(2).Value, "YYYY-MM-DD") & " " & Format(Now, "hh:mm:ss")
        
        For i = 1 To .MaxRows
            ProgressBar.Value = (i / spdView1.MaxRows) * 100
            DoEvents
            
            .Row = i
            
            ReDim sValue(11)
            '-------------------------------------------------------------------------------------
            ' 1) TB_입출고에 출고 설정
            ' 2) SCANOUTPUT_LOG_TB 저장
            ' 3) SCANOUTPUT_TB 삭제
            '-------------------------------------------------------------------------------------
            .Col = 8:  sValue(0) = Trim(.Text) & ""             ' 0 SCAN_DATE
            .Col = 9:  sValue(1) = .Text & ""                   ' 1 PDA_NO
                       sValue(2) = 가맹점코드                   ' 2 가맹점코드
            .Col = 1:  sValue(3) = Replace(.Text, "-", "") & "" ' 3 TAG_NO
            .Col = 4:  sValue(4) = Trim(.Text) & ""             ' 4 SCAN_FLAG1
            .Col = 6:  sValue(5) = Trim(.Text) & ""             ' 5 SCAN_FLAG2
            .Col = 10: sValue(6) = Trim(.Text) & ""             ' 6 OUT_FLAG
                       sValue(7) = 출고일자                     ' 7 지사출고일자
                       sValue(8) = 지사출고번호                 ' 8 OUT_COUNT
                       sValue(9) = ""                           ' 9 수기 일일출고에서 사용하는 필드
                       sValue(10) = Format(dtInput(0).Value, "YYYY-MM-DD 00:00:01")  ' 스케너 스켄 일자
                       sValue(11) = Format(dtInput(1).Value, "YYYY-MM-DD 23:59:59")  ' 스케너 스켄 일자
                       
            Call ExecPro("SP_03015_EDIT", sValue(), Err_Num, Err_Dec)
            
            If Err_Num <> 0 Then
                txtErrMsg.Text = txtErrMsg.Text & Err_Dec & vbNewLine
            End If
            
        Next i
        
        
        spdView1.MaxRows = 0
    End With
    
    ProgressBar.Visible = False
End Sub


Private Sub Data_Action()
    Dim iRow        As Long
    Dim vText       As Variant
    Dim 가맹점코드  As String
    Dim Query       As String

    On Error GoTo ErrRtn

    If Format(dtInput(2).Value, "yyyy-MM-dd") < Format(Date, "yyyy-MM-dd") Then
        Rtn = MsgBox("선택일자가 이전 일자 입니다. 그래도 출고 처리 하시겠습니까?", vbCritical + vbYesNo + vbDefaultButton2, "확인")
        If Rtn = vbNo Then Exit Sub
    End If

    spdView.Enabled = False
    cmdBtn(7).Enabled = False
    Rtn = MsgBox("출고처리 하겠습니까?", vbQuestion + vbYesNo + vbDefaultButton2, "확인")
    
    If Rtn = vbNo Then
        spdView.Enabled = True
        cmdBtn(8).Enabled = True
        cmdBtn(7).Enabled = True
        
        ' 종료 버튼을 활성/비활성화 한다.
        P_00000.CommandBars.Item(2).Controls(7).Enabled = True
        Exit Sub
    End If
    
    ReDim sSpValue(7)
    ' 작업 시작 시간을 기록 한다.
    sSpValue(4) = Now
    '----------------------------------------------------------------------------------------------------
    ' 지수출고 횟수를 구한다. - TB_입출고 테이블 '지사출고번호'에 저장
    '----------------------------------------------------------------------------------------------------
    Query = "SELECT ISNULL(MAX(OUT_COUNT),0) + 1"
    Query = Query & " FROM SCANOUTPUT_LOG_TB"
    Query = Query & " WHERE SUBSTRING(OUT_DATE,1,10) = '" & Format(dtInput(2).Value, "YYYY-MM-DD") & "'"
    Set RS01 = New ADODB.Recordset
    RS01.Open Query, ADOCon, adOpenForwardOnly, adLockReadOnly
        
    지사출고번호 = RS01(0)
        
    '--------------------------------------------------------------------------------------------------------
    ' 작업 시작 로그를 저장한다.
    sSpValue(0) = Store.Code: sSpValue(1) = dtInput(2).Value:   sSpValue(2) = 지사출고번호
    sSpValue(3) = "000000":   sSpValue(5) = Now:                sSpValue(6) = "1":    sSpValue(7) = "정상 처리"
    ' sValue(4) = Now  <- 위쪽에서 기록
    Call P_03015_Data(sSpValue)
    '--------------------------------------------------------------------------------------------------------
    
    
    RS01.Close
    Set RS01 = Nothing
    
    For iRow = 1 To spdView.MaxRows
        spdView.Row = iRow
        
        spdView.GetText 1, iRow, vText
        If CStr(vText) = "1" Then
            spdView.Col = 4: sSpValue(6) = spdView.Text & ""
            spdView.Col = 2: 가맹점코드 = spdView.Text & ""
            
            spdView.SetSelection 1, iRow, 1, iRow
    
            ' 작업 시작 시간을 기록 한다.
            sSpValue(4) = Now
    
            Call Data_Display2(가맹점코드)
            Call Data_Update   '
            
            '--------------------------------------------------------------------------------------------------------
            ' 작업 시작 로그를 저장한다.
            sSpValue(0) = Store.Code: sSpValue(1) = dtInput(2).Value:   sSpValue(2) = 지사출고번호
            sSpValue(3) = 가맹점코드: sSpValue(5) = Now:                sSpValue(7) = "정상 처리"
            ' sValue(4) = Now  <- 위쪽에서 기록
            Call P_03015_Data(sSpValue)
            '--------------------------------------------------------------------------------------------------------
        End If
    Next iRow
    
    Call Data_Display      '
    
    cmdBtn(8).Enabled = True
    spdView.Enabled = True
    cmdBtn(7).Enabled = True
    
    Exit Sub
    
ErrRtn:
    Me.MousePointer = 0
    cmdBtn(8).Enabled = True
    cmdBtn(7).Enabled = True
    spdView.Enabled = True
    
    ' 종료 버튼을 활성/비활성화 한다.
    P_00000.CommandBars.Item(2).Controls(7).Enabled = True
    
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
        
End Sub

