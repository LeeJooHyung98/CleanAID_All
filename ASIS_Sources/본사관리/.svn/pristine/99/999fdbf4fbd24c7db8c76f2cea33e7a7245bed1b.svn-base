Private Sub Data_Display2(가맹점코드 As String)
    Dim i As Integer
    
    On Error GoTo ErrRtn
    
    pnlProg.Visible = True
    DoEvents
    
    For i = 1 To 8
        txtNum(i).Value = 0
    Next i
    
    ReDim sValue(2)
    
    '------------------------------------------------------------
    ' 지사 입고처리 - SP_03015_01
    '------------------------------------------------------------
    sValue(0) = 가맹점코드 'Mid(cboInput.Text, 2, 6)
    sValue(1) = Format(dtInput(0).Value, "YYYY-MM-DD")
    sValue(2) = Format(dtInput(1).Value, "YYYY-MM-DD")
        
    If HeadOffice = MASTER_OFFICE_CODE Then
        If DBOpen_Master(Mid(cboOffice.Text, 2, 4)) = False Then Exit Sub

        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecProMaster("SP_03015_01", sValue(), Err_Num, Err_Dec)
    Else
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecPro("SP_03015_01", sValue(), Err_Num, Err_Dec)
    End If
            
    With spdView1
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = 1: .Text = Format(RS01!TAG_NO, "000-00-0000") 'Left(RS01!TAG_NO, 5) & "-" & Mid(RS01!TAG_NO, 6, 4) '
            .Col = 2: .Text = RS01!의류코드 & ""  '
            .Col = 3: .Text = RS01!의류명 & ""    '
            
            .Col = 4: .Text = RS01!SCAN_FLAG1 & "" '
            .Col = 5: .Text = RS01!물품 & "" '
            
            .Col = 6: .Text = RS01!SCAN_FLAG2 & "" '
            .Col = 7: .Text = RS01!상태 & "" '
            
            .Col = 8: .Text = RS01!SCAN_DATE & "" '
            .Col = 9: .Text = RS01!PDA_NO & ""    '
            
            If Len(Trim(RS01!의류코드)) <> 4 Then
                .Col = 10: .Text = "N"
            Else
                .Col = 10: .Text = "Y"
            End If
            
            If RS01!물품 = "의류" Then txtNum(1).Value = txtNum(1).Value + 1
            If RS01!물품 = "소품" Then txtNum(2).Value = txtNum(2).Value + 1
            If RS01!물품 = "기타" Then txtNum(3).Value = txtNum(3).Value + 1
            
            If RS01!상태 = "정상" Then txtNum(4).Value = txtNum(4).Value + 1
            If RS01!상태 = "반품" Then txtNum(5).Value = txtNum(5).Value + 1
            If RS01!상태 = "확인" Then txtNum(6).Value = txtNum(6).Value + 1
            If RS01!상태 = "품명" Then txtNum(7).Value = txtNum(7).Value + 1
                        
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
        
        .Redraw = True
    End With
    
    pnlProg.Visible = False
    
    Exit Sub
    
ErrRtn:
    pnlProg.Visible = False
End Sub

Private Sub spdView_Click(ByVal Col As Long, ByVal Row As Long)
    Dim 가맹점코드 As String
    
    If Row <= 0 Then Exit Sub
    If Col <= 1 Then Exit Sub
    
    spdView.Enabled = False
    
    spdView.Row = Row
    spdView.Col = 2: 가맹점코드 = spdView.Text & ""
    
    Call Data_Display2(가맹점코드)
    
    spdView.Enabled = True
End Sub

