Private Sub Data_Display()
    On Error GoTo ErrRtn

    Dim i As Integer
    
    ReDim sValue(3)
    
    sValue(0) = Mid(cboOffice.Text, 2, 4)
    sValue(1) = ""
    sValue(2) = Format(dtInput.Value, "YYYY-MM-01")
    sValue(3) = Format(dtInput.Value, "YYYY-MM-31")
    
    
    If HeadOffice = MASTER_OFFICE_CODE Then
        If DBOpen_Master(sValue(0)) = False Then Exit Sub
        
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecProMaster("SP_04009_N_01", sValue(), Err_Num, Err_Dec)
    Else
        Set RS01 = New ADODB.Recordset
        Set RS01 = ExecPro("SP_04009_N_01", sValue(), Err_Num, Err_Dec)
    End If
    
    With spdView
        .MaxRows = 0
        .Redraw = False
        
        Do Until RS01.EOF
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = -1
            .BackColor = IIf(.Row Mod 2, vbWhite, glbGray)
            
            .Col = 1:  .Text = RS01!가맹점코드 & ""               ' 1
            .Col = 2:  .Text = RS01!가맹점구분명 & ""                 ' 2
            .Col = 3:  .Text = RS01!가맹점명 & ""                 ' 3
            .Col = 4:  .Text = RS01!영업일수 & ""                 ' 3
            .Col = 5:  .Text = RS01!지사금액 & ""                 ' 4
            .Col = 6:  .Text = RS01!가맹점금액 & ""               ' 5
            
            .Col = 7:  .Text = RS01!로열티금액1 & ""                      ' 6
            .Col = 8:  .Text = RS01!로열티금액2 & ""                 ' 7
            .Col = 9:   .Text = RS01!지사차감후금액 & ""                 ' 7
            
            .Col = 10: .Text = RS01!수수료승인금액 & ""                 ' 7
            .Col = 11: .Text = RS01!수수료취소금액 & ""                 ' 7
            .Col = 12: .Text = RS01!수수료지원금액 & ""                 ' 7
            .Col = 13: .Text = RS01!본사정산분 & ""                 ' 7
            
            .Col = 14:  .Text = RS01!세탁반품환불건수 & ""                 ' 6
            .Col = 15:  .Text = RS01!세탁반품환불지사금액 & ""                 ' 7
            
            .Col = 16:  .Text = RS01!접수수량 & ""                 ' 6
            .Col = 17:  .Text = RS01!출고수량 & ""                 ' 7
            .Col = 18:  .Text = RS01!접수금액 & ""                 ' 8
            .Col = 19:  .Text = RS01!현금입금 + RS01!카드금액 & "" ' 9
            
            If RS01!접수수량 = 0 Then
                .Col = 20: .Text = 0 & ""   '10
                .Col = 21: .Text = 0 & ""   '11
                .Col = 22: .Text = 0 & ""   '12
            Else
                .Col = 20: .Text = RS01!접수금액 / RS01!접수수량 & ""   '10
                .Col = 21: .Text = RS01!지사금액 / RS01!접수수량 & ""   '11
                .Col = 22: .Text = RS01!가맹점금액 / RS01!접수수량 & "" '12
            End If
            
            .Col = 23: .Text = RS01!현금입금 & ""                 '10
            .Col = 24: .Text = RS01!카드금액 & ""                 '11
            .Col = 25: .Text = RS01!카드건수 & ""                 '12
            .Col = 26: .Text = RS01!쿠폰금액 & ""                 '13
            .Col = 27: .Text = RS01!쿠폰건수 & ""                 '14
            .Col = 28: .Text = RS01!발생마일리지 & ""             '15
            .Col = 29: .Text = RS01!사용마일리지 & ""             '16
            .Col = 30: .Text = RS01!삭제마일리지 & ""             '17
            
            .Col = 31: .Text = RS01!미수카드금액 & ""             '19
            .Col = 32: .Text = RS01!미수카드건수 & ""             '19
            .Col = 33: .Text = RS01!카드취소금액 & ""             '19
            .Col = 34: .Text = RS01!카드취소건수 & ""             '19
            
            .Col = 35: .Text = RS01!반품환불금액 & ""             '18
            .Col = 36: .Text = RS01!반품환불건수 & ""             '19
            .Col = 37: .Text = RS01!세탁환불금액 & ""             '20
            .Col = 38: .Text = RS01!세탁환불건수 & ""             '21
            .Col = 39: .Text = RS01!재세탁수량 & ""               '22
            .Col = 40: .Text = RS01!수선금액 & ""                 '23
            .Col = 41: .Text = RS01!수선수량 & ""                 '24
                        
            
            RS01.MoveNext
        Loop
        RS01.Close
        Set RS01 = Nothing
        
        
        If .MaxRows > 0 Then
        
            ' 합계 출력
            Dim nCol    As Long
            Dim dblCnt(4)   As Double
            For nCol = 5 To .MaxCols
                Select Case nCol
                    Case 5: dblCnt(2) = SpreadSum(spdView, 3, nCol)
                    Case 6: dblCnt(3) = SpreadSum(spdView, -1, nCol)
                    Case 18: dblCnt(1) = SpreadSum(spdView, -1, nCol)
                    Case 16:  dblCnt(0) = SpreadSum(spdView, -1, nCol)
                    Case 20: .SetText nCol, .MaxRows, CVar(dblCnt(1) / dblCnt(0))
                    Case 21: .SetText nCol, .MaxRows, CVar(dblCnt(2) / dblCnt(0))
                    Case 22: .SetText nCol, .MaxRows, CVar(dblCnt(3) / dblCnt(0))
                    Case Else: Call SpreadSum(spdView, -1, nCol)
                End Select
            Next nCol
        End If
        
        ' 이마트 매장 구분 출력
        Call Data_Sum
        
        .Redraw = True
    End With
        
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub

