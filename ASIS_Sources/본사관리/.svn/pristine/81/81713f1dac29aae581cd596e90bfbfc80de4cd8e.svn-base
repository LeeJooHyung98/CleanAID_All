Private Sub Data_Sum()
    Dim vText       As Variant
    Dim nRow        As Long
    Dim nCol        As Long
    Dim dValue(1, 100)   As Double
    Dim sGubun(1)  As String
    Dim iGubun      As Integer
    
    On Error GoTo ErrRtn
    
    sGubun(0) = "이마트":   sGubun(1) = "기타"
    
    Erase dValue
    
    With spdView
        If .MaxRows > 0 Then
            For nCol = 4 To .MaxCols
                For nRow = 1 To .MaxRows - 1
                    
                    .GetText nCol, nRow, vText
                    If IsNumeric(vText) Then
                        .GetText 2, nRow, vText
                        
                        iGubun = IIf(InStr(CStr(vText), sGubun(0)) > 0, 0, 1)
                        
                        ' 이마트 일경우(0),나머지 (1)
                        .GetText nCol, nRow, vText
                        dValue(iGubun, nCol) = dValue(iGubun, nCol) + CDbl(vText)
                    End If
                
                Next nRow
            Next nCol
        
        End If
        
        .MaxRows = .MaxRows + 2

        .Row = .MaxRows - 2:        .Row2 = .MaxRows
        .Col = 1:                   .Col2 = .MaxCols
        .BlockMode = True:          .BackColor = &HC0FFC0:      .BlockMode = False
        
        
        For nRow = 0 To 1
            For nCol = 4 To .MaxCols
                If nCol = 4 Then .SetText 3, .MaxRows - (1 - nRow), CVar(IIf(nRow = 0, "이마트 매장 합계", "기타 매장 합계"))
                .SetText nCol, .MaxRows - (1 - nRow), CVar(dValue(nRow, nCol))
            Next nCol
        Next nRow
        
    End With
        
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.Description)
End Sub
