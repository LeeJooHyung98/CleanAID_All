Private Sub PrintDesc()
    Dim i As Integer
    Dim TempText As String
    Dim TempFP As String
    Dim TempFile As String
    Dim FHandel As Integer
    
    FHandle = FreeFile
    
    TempFP = GetIniStr("REPORT", "TempPath", "", m_iniFile)
    TempFile = TempFP & "\Temp.txt"
    
    
    Open TempFile For Output As #FHandle
    
    TempText = ""
    
    For i = 1 To spdView.MaxRows - 1
        spdView.Row = i
        
        spdView.Col = 1
        TempText = LeftH(spdView.Text & Space(4), 4)
        spdView.Col = 2
        TempText = TempText & LeftH(spdView.Text & Space(16), 16)
        spdView.Col = 3
        TempText = TempText & RightH(Space(12) & spdView.Text, 12)
        spdView.Col = 4
        TempText = TempText & RightH(Space(8) & spdView.Text, 8)
        spdView.Col = 5
        TempText = TempText & RightH(Space(12) & spdView.Text, 12)
        spdView.Col = 6
        TempText = TempText & RightH(Space(8) & spdView.Text, 8)
        spdView.Col = 7
        TempText = TempText & RightH(Space(8) & spdView.Text, 8)
        spdView.Col = 8
        TempText = TempText & RightH(Space(8) & spdView.Text, 8)
        spdView.Col = 9
        TempText = TempText & RightH(Space(8) & spdView.Text, 8) & Space(2)
        spdView.Col = 10
        TempText = TempText & LeftH(spdView.Text & Space(5), 5) & " ~ "
        spdView.Col = 11
        TempText = TempText & LeftH(spdView.Text & Space(5), 5)
        
        Print #FHandle, TempText
    Next i
    
    Close #FHandle
End Sub
