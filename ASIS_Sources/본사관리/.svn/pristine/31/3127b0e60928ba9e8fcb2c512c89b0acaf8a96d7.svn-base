    spdView2.MaxCols = RS01.Fields.Count
    spdView2.MaxRows = RS01.RecordCount
    
    'Call spdDisplay2(RS01)
    Call fpSpread_Display(spdView2, RS01)
    Call GetColWidth(REG_App, Me.Name, spdView2)
End Sub

