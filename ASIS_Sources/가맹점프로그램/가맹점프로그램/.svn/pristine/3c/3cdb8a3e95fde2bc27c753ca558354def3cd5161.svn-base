Private Sub btnDel_Click()
    Dim intCount As Integer
      
    intCount = Len(txtTAGNo.Text)
    
    If intCount < 1 Then Exit Sub
    
    intCount = intCount - 1
    
    txtTAGNo.Text = Mid(txtTAGNo.Text, 1, intCount)
End Sub

