Private Sub Data_Display(iCount As Integer)
'    On Error GoTo ErrRtn
'
'    If Server_Connection(HostCon) = False Then
'        MsgBox "DB 서버와 접속이 되지 않아 본사출고 자료를 조회할수 없습니다.", vbCritical, "확인"
'
'        Exit Sub
'    End If
    
    Dim 지사코드 As String
    
    On Error GoTo ErrRtn
    
    지사코드 = "LAUNDRY" & Left(cboOffice.Text, 4)
    
    If Server_Connection(HostCon, 지사코드) = False Then
        MsgBox "DB 서버와 접속이 되지 않아 본사출고 자료를 조회할수 없습니다.", vbCritical, "확인"
        
        Exit Sub
    End If
    
    lblCount.Caption = "0"
    pnlProg.Visible = True
    DoEvents
    
    For i = 0 To 8
        txtNum(i).Value = 0
    Next i
    
    '-----------------------------------------------------------------------------------------------
    ' SCANOUTPUT_LOG_TB
    '-----------------------------------------------------------------------------------------------
    Query = "SELECT * FROM SCANOUTPUT_LOG_TB"
    Query = Query & " WHERE STORE_CD                 = '" & 가맹점정보.가맹점코드 & "'"
    Query = Query & "   AND SUBSTRING(OUT_DATE,1,10) = '" & Format(dtpDay.Value, "YYYY-MM-DD") & "'"
    Query = Query & "   AND OUT_COUNT                =  " & iCount
    Query = Query & " ORDER BY TAG_NO ASC"
    Set ADORs = New ADODB.RecordSet
    ADORs.Open Query, HostCon, adOpenForwardOnly, adLockReadOnly
    
    With sprGrid
        .MaxRows = 0
        .ReDraw = False
        
        Do Until ADORs.EOF
            Query = "SELECT    B.접수번호"
            Query = Query & ", B.출고일자"
            Query = Query & ", ISNULL(B.가맹점입고일자, '') AS 가맹점입고일자"
            Query = Query & ", B.지사출고일자"
            Query = Query & ", B.지사출고번호"
            Query = Query & ", B.지사출고물품"
            Query = Query & ", B.지사출고상태"
            Query = Query & ", B.의류명"
            Query = Query & ", A.택번호"
            Query = Query & ", A.접수일자"
            Query = Query & ", B.부모택번호"
            Query = Query & ", B.색상"
            Query = Query & ", B.무늬"
            Query = Query & ", B.내용"
            Query = Query & ", B.금액"
            Query = Query & ", B.결제여부"
            Query = Query & ", B.상표"
            Query = Query & ", C.성명"
            Query = Query & ", C.휴대전화"
            Query = Query & ", C.전화번호"
            Query = Query & ", B.가맹점입고구분"
            Query = Query & " FROM (SELECT 택번호"
            Query = Query & "            , MAX(접수일자) AS 접수일자"
            Query = Query & "       FROM TB_입출고"
            Query = Query & "       WHERE 접수일자 <= '" & Format(dtpDay.Value, "YYYY-MM-DD") & "'"
'            Query = Query & "         AND 가맹점코드 = '" & 가맹점정보.가맹점코드 & "'"
            Query = Query & "         AND 택번호    = '" & ADORs!Tag_No & "'"
            Query = Query & "         AND 판매취소 <> 'Y'"
            Query = Query & "       GROUP BY 택번호"
            Query = Query & "      ) AS A LEFT OUTER JOIN TB_입출고   AS B ON A.택번호   = B.택번호"
            Query = Query & "                                             AND A.접수일자 = B.접수일자"
            Query = Query & "                                             AND B.판매취소 <> 'Y'"
            Query = Query & "             LEFT OUTER JOIN TB_고객정보 AS C ON B.고객코드 = C.고객코드"
            Set SUBRs = New ADODB.RecordSet
            SUBRs.Open Query, ADOCon, adOpenForwardOnly, adLockReadOnly
            
            Debug.Print Now
                                             
                                             
            ' 출고한 품목이 없을 경우
            If SUBRs.EOF Then
                .MaxRows = .MaxRows + 1
                .Row = .MaxRows
            
                .Col = 1:  .Text = "1"                                           ' 1
                .Col = 2:  .Text = Format(ADORs!Tag_No, "000-00-0000")           ' 2
                .Col = 3:  .Text = ""                                            ' 3
                .Col = 4:  .Text = iCount & ""                                   ' 4 지사출고번호
                
                Select Case ADORs!SCAN_FLAG1
                    Case "1": .Col = 5: .Text = "[" & ADORs!SCAN_FLAG1 & "]의류" ' 5
                    Case "2": .Col = 5: .Text = "[" & ADORs!SCAN_FLAG1 & "]소품" ' 5
                    Case "3": .Col = 5: .Text = "[" & ADORs!SCAN_FLAG1 & "]기타" ' 5
                End Select
                
                Select Case ADORs!SCAN_FLAG2
                    Case "1": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]정상" ' 6
                    Case "2": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]반품" ' 6
                    Case "3": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]확인" ' 6
                    Case "4": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]품명" ' 6
                End Select
                
                .Col = 7:  .Text = ""                                            ' 7
                .Col = 8:  .Text = ""                                            ' 8
                .Col = 9:  .Text = ""                                            ' 9
                .Col = 10: .Text = ""                            '10
                .Col = 11: .Text = ""                              '11
                .Col = 12: .Text = ""                                '12
                .Col = 13: .Text = ""                                '13
                .Col = 14: .Text = ""                                '14
                .Col = 15: .Text = ""                                '15
                .Col = 16: .Text = ""                                '16
                .Col = 17: .Text = ADORs!OUT_DATE & ""                           '17 SUBRs!지사출고일자 & ""
                .Col = 18: .Text = ""                            '18
                .Col = 19: .Text = "0"                                       '19
                .Col = 20: .Text = ""                                       '19
                .Col = 21: .Text = ""                                       '19
                
            Else
                .MaxRows = .MaxRows + 1
                .Row = .MaxRows
            
                .Col = 1:  .Text = "1"                                           ' 1
                .Col = 2:  .Text = Format(SUBRs!택번호, "000-00-0000")           ' 2
                .Col = 3:  .Text = Format(SUBRs!부모택번호, "000-00-0000")       ' 3
                .Col = 4:  .Text = iCount & ""                                   ' 4 지사출고번호
                
                Select Case ADORs!SCAN_FLAG1
                    Case "1": .Col = 5: .Text = "[" & ADORs!SCAN_FLAG1 & "]의류" ' 5
                    Case "2": .Col = 5: .Text = "[" & ADORs!SCAN_FLAG1 & "]소품" ' 5
                    Case "3": .Col = 5: .Text = "[" & ADORs!SCAN_FLAG1 & "]기타" ' 5
                End Select
                
                Select Case ADORs!SCAN_FLAG2
                    Case "1": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]정상" ' 6
                    Case "2": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]반품" ' 6
                    Case "3": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]확인" ' 6
                    Case "4": .Col = 6: .Text = "[" & ADORs!SCAN_FLAG2 & "]품명" ' 6
                End Select
                
                .Col = 7:  .Text = SUBRs!접수일자 & ""                           ' 7
                .Col = 8:  .Text = SUBRs!성명 & ""                               ' 8
                .Col = 9:  .Text = SUBRs!전화번호 & ""                           ' 9
                .Col = 10: .Text = SUBRs!휴대전화 & ""                           '10
                .Col = 11: .Text = SUBRs!의류명 & ""                             '11
                .Col = 12: .Text = SUBRs!색상 & ""                               '12
                .Col = 13: .Text = SUBRs!무늬 & ""                               '13
                .Col = 14: .Text = SUBRs!내용 & ""                               '14
                .Col = 15: .Text = SUBRs!금액 & ""                               '15
                .Col = 16: .Text = SUBRs!상표 & ""                               '16
                .Col = 17: .Text = ADORs!OUT_DATE & ""                           '17 SUBRs!지사출고일자 & ""
                .Col = 18: .Text = SUBRs!접수번호 & ""                           '18
                    
                If SUBRs!가맹점입고일자 = "" Or SUBRs!가맹점입고일자 = "00:00:00" Then
                    .Col = 19: .Text = "0"                                       '19
                Else
                    .Col = 1:  .Text = "0"                                       ' 1
                    .Col = 19: .Text = "1"                                       '19

                    .Row = .MaxRows: .Row2 = .MaxRows
                    .Col = 1: .Col2 = .MaxCols
                    .BlockMode = True
                    .BackColor = vbYellow
                    .BlockMode = False
                End If
                
                .Col = 20: .Text = SUBRs!가맹점입고구분 & ""                           '18
                .Col = 21: .Text = SUBRs!가맹점입고일자 & ""                           '18
                
                Rtn = InStr(SUBRs!내용 & "", "재")
                
                If Rtn > 0 Then txtNum(8).Value = txtNum(8).Value + 1
            End If
            SUBRs.Close
            Set SUBRs = Nothing
            
            If ADORs!SCAN_FLAG1 = "1" Then txtNum(1).Value = txtNum(1).Value + 1 '의류
            If ADORs!SCAN_FLAG1 = "2" Then txtNum(2).Value = txtNum(2).Value + 1 '소품
            If ADORs!SCAN_FLAG1 = "3" Then txtNum(3).Value = txtNum(3).Value + 1 '기타
            
            If ADORs!SCAN_FLAG2 = "1" Then txtNum(4).Value = txtNum(4).Value + 1 '정상
            If ADORs!SCAN_FLAG2 = "2" Then txtNum(5).Value = txtNum(5).Value + 1 '반품
            If ADORs!SCAN_FLAG2 = "3" Then txtNum(6).Value = txtNum(6).Value + 1 '확인
            If ADORs!SCAN_FLAG2 = "4" Then txtNum(7).Value = txtNum(7).Value + 1 '품명
                
                
            lblCount.Caption = CLng(lblCount.Caption) + 1
            
            DoEvents
            
            ADORs.MoveNext
        Loop
        ADORs.Close
        Set ADORs = Nothing
        
        txtNum(0).Value = .MaxRows '전체수량
        
        .ReDraw = True
    End With
    
    pnlProg.Visible = False
    HostCon.Close:  Set HostCon = Nothing
    
    Exit Sub
    
ErrRtn:
    pnlProg.Visible = False
    HostCon.Close:  Set HostCon = Nothing

    Call Error_Msg("", Err.Source, Err.Number, Err.description)
End Sub

Private Sub cboOffice_Click()
    cmdList_Click
End Sub

Private Sub cmdBtn_Click(Index As Integer)
    On Error GoTo ErrRtn
    
    Select Case Index
        Case 2
        
        
            
            With sprGrid
                If .DataRowCnt <= 0 Then
                    MsgBox "왼쪽의 내용을 클릭한 후 작업하여 주십시요", vbInformation, "확인"
                    Exit Sub
                End If
            
                Ret = MsgBox("가맹점 입고처리하시겠습니까?", vbQuestion + vbYesNo + vbDefaultButton2, "확인")
                If Ret = vbNo Then Exit Sub
                
                For i = 1 To .MaxRows
                    .Row = i
                    
                    .Col = 1
                    If .Text = "1" Then
                        '------------------------------------------
                        ' 출고된 것중 가맹점 미입고된 것만...
                        '------------------------------------------
                        .Col = 19
                        If .Text = "0" Then
                                       Query = "UPDATE TB_입출고 SET 가맹점입고일자 = '" & Format(Now, "YYYY-MM-DD hh:mm:ss") & "'"
                                       Query = Query & "           , 가맹점입고구분 = '자동'"
                            .Col = 17: Query = Query & "           , 지사출고일자   = '" & Trim(.Text) & "'"
                            .Col = 4:  Query = Query & "           , 지사출고번호   =  " & .Value
                            .Col = 5:  Query = Query & "           , 지사출고물품   = '" & Mid(.Text, 2, 1) & "'"
                            .Col = 6:  Query = Query & "           , 지사출고상태   = '" & Mid(.Text, 2, 1) & "'"
                                       Query = Query & "           , 본사전송여부   = ''"
                            .Col = 7:  Query = Query & " WHERE 접수일자 = '" & .Text & "'"
                            .Col = 2:  Query = Query & "   AND 택번호   = '" & Replace(.Text, "-", "") & "'"
                            Debug.Print Query
                            'Call Error_Msg("", "내용", "9999", Query)
                            ADOCon.Execute Query
                        End If
                    End If
                Next i
            End With
            
            Call sprCount_Click(1, sprCount.ActiveRow)
            
            MsgBox "가맹점 입고처리하였습니다.", vbInformation, "확인"
            
        Case 3:
            Call Export_Excel(frmMain.cdgExcel, sprGrid)
        
        Case 4
            Rtn = MsgBox("출력 미리보기를 하시겠습니까?", vbQuestion + vbYesNo, "출력")
            
            If Rtn = vbYes Then
                Call Data_Print(True)
            Else
                Call Data_Print(False)
            End If
            
        Case 5: Unload Me
    End Select
    
    Exit Sub
    
ErrRtn:
    pnlProg.Visible = False

    Call Error_Msg("", Err.Source, Err.Number, Err.description)
End Sub

Private Sub Data_Print(Print_PreView As Boolean)
    On Error GoTo ErrRtn
    
    If sprGrid.MaxRows = 0 Then Exit Sub

    If Dir(AppPath & "XML", vbDirectory) = "" Then
        MkDir AppPath & "XML"
    End If

    Open AppPath & "XML\본사출고현황.XML" For Output As #1
    
    Print #1, "<?xml version=""1.0"" encoding=""EUC-KR""?>"
    Print #1, "<root>"
    
          XML = "    <조건>"
    XML = XML & "        <검색조건>출고일자 : " & Format(dtpDay.Value, "YYYY-MM-DD") & "</검색조건>"
    XML = XML & "        <가맹점>크린에이드 - " & Func_Replace(가맹점정보.가맹점명) & "</가맹점>"
    XML = XML & "        <의류>의류 : " & txtNum(1).Text & " 점</의류>"
