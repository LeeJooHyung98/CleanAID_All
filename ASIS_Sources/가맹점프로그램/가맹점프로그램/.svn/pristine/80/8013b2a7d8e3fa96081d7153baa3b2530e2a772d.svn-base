Private Sub cmdList_Click()
    Dim 지사코드 As String
    
    On Error GoTo ErrRtn
    
    DoEvents
    
    지사코드 = "LAUNDRY" & Left(cboOffice.Text, 4)
    
    If Len(Trim(지사코드)) <> 11 Then Exit Sub
    
    
    If Server_Connection(HostCon, 지사코드) = True Then
        sprGrid.MaxRows = 0
        
        For i = 0 To 8
            txtNum(i).Value = 0
        Next i
            
'        frmMain.CommandBars.StatusBar.IdleText = "진행 1"
'        DoEvents
        '
        Query = "SELECT    OUT_COUNT"
        Query = Query & ", COUNT(TAG_NO)"
        Query = Query & " FROM SCANOUTPUT_LOG_TB"
        Query = Query & " WHERE STORE_CD                 = '" & 가맹점정보.가맹점코드 & "'"
        Query = Query & "   AND SUBSTRING(OUT_DATE,1,10) = '" & Format(dtpDay.Value, "YYYY-MM-DD") & "'"
        Query = Query & " GROUP BY OUT_COUNT"
        Set ADORs = New ADODB.RecordSet
        ADORs.Open Query, HostCon, adOpenForwardOnly, adLockReadOnly
        
 '       frmMain.CommandBars.StatusBar.IdleText = "진행 2"
 '       DoEvents
        
    
        With sprCount
            .MaxRows = 0
            .ReDraw = False
            
            Do Until ADORs.EOF
                .MaxRows = .MaxRows + 1
                .Row = .MaxRows
            
                .Col = 1: .Text = ADORs(0) & ""
                .Col = 2: .Text = ADORs(1) & ""
                
                '---------------------------------------------------------------------------------------------------
                Query = "SELECT ISNULL(COUNT(택번호),0)"
                Query = Query & " FROM TB_입출고"
                Query = Query & " WHERE 지사출고일자 BETWEEN '" & Format(dtpDay.Value, "YYYY-MM-DD") & " 00:00:00'"
                Query = Query & "           and '" & Format(dtpDay.Value, "YYYY-MM-DD") & " 23:59:59'"
                Query = Query & "   AND 지사출고번호                 =  " & ADORs(0)
                Query = Query & "   AND 판매취소 <> 'Y'"
                Set SUBRs = New ADODB.RecordSet
                SUBRs.Open Query, ADOCon, adOpenForwardOnly, adLockReadOnly
                
                If SUBRs.EOF Then
                    .Col = 3: .Text = ""
                Else
                    .Col = 3: .Text = SUBRs(0) & ""
                End If
                SUBRs.Close
                Set SUBRs = Nothing
                
                ADORs.MoveNext
            Loop
            ADORs.Close
            Set ADORs = Nothing
            
            .ReDraw = True
        End With
    
        HostCon.Close
        Set HostCon = Nothing
    Else
        MsgBox "본사 서버와 연결할 수 없습니다.  인터넷을 확인 하여 주십시요.", vbInformation, "확인"
        Exit Sub
    End If
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.description & "  " & 지사코드)
End Sub

