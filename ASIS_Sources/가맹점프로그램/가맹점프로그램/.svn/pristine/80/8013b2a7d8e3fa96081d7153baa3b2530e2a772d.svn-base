Private Sub Form_Load()
    Dim bSear As Boolean
    
    m_ACTIVE = False
    
    With sprCount
        .MaxRows = 0
        .RowHeight(-1) = 14
        
        'Spread 8 - 디자인
        .HighlightHeaders = HighlightHeadersOff
        .AppearanceStyle = AppearanceStyleEnhanced
        .ScrollBarStyle = ScrollBarStyleVisualStyle
        
        '선택된 Row
        .SelBackColor = &HFFFFC0 '황색 ^^
        .SelForeColor = &H0&     '검은글씨
        .OperationMode = OperationModeSingle
        
        'Init the User Sort
        .UserColAction = UserColActionSort
    End With
    
    With sprGrid
        .MaxRows = 0
        .RowHeight(-1) = 14
        
        'Spread 8 - 디자인
        .HighlightHeaders = HighlightHeadersOff
        .AppearanceStyle = AppearanceStyleEnhanced
        .ScrollBarStyle = ScrollBarStyleVisualStyle
        
        '선택된 Row
        .SelBackColor = &HFFFFC0 '황색 ^^
        .SelForeColor = &H0&     '검은글씨
        .OperationMode = OperationModeNormal
        
        'Init the User Sort
        .UserColAction = UserColActionSort
    End With
    
    dtpDay.Value = Format(Date, "YYYY-MM-DD")
    
    '-----------------------------------------------------------
    '
    '-----------------------------------------------------------
'    Query = "SELECT DISTINCT 지사코드 FROM TB_입출고  where 가맹점입고일자 =  '' "
    Query = ""
    Query = Query & " select AA.지사코드, bb.지사명 "
    Query = Query & " from (SELECT DISTINCT 지사코드 FROM TB_입출고  where 가맹점입고일자 =  '') AA "
    Query = Query & "  left outer join tb_지사 BB "
    Query = Query & "  on AA.지사코드 = bb.지사코드 "
    Set ADORs = New ADODB.RecordSet
    ADORs.Open Query, ADOCon, adOpenForwardOnly, adLockReadOnly
    
    
    With cboOffice
        .Clear
        bSear = False
        
        Do Until ADORs.EOF
            .AddItem ADORs!지사코드 & " : " & ADORs!지사명 & ""
            
            If ADORs!지사코드 & "" = 가맹점정보.지사코드 Then
                bSear = True
            
            End If
            
            ADORs.MoveNext
        Loop
        ADORs.Close:    Set ADORs = Nothing
        
        ' 현재 지사가 없을 경우(접수를 한품목도 안한 경우)
        If bSear = False Then .AddItem 가맹점정보.지사코드, 0
        
        If .ListCount > 0 Then
            
            For i = 0 To .ListCount - 1
                If Left(.List(i), 4) = 가맹점정보.지사코드 Then
                    .ListIndex = i
                    Exit For
                End If
            Next i
        End If
        
    End With
End Sub

