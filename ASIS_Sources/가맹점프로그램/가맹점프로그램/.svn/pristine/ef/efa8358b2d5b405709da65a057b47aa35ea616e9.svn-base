    If InStr(Rate, "ERROR") > 0 Then
        MsgBox (Replace(Rate, "ERROR ", ""))
    Else
        EventCode = txtCode.Text
        Unload Me
    End If
End Sub

Private Sub txtCode_KeyPress(KeyAscii As Integer)
