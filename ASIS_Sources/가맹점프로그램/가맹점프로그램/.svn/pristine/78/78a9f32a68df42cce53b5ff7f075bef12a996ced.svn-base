Private Sub btnTerm_Click(Index As Integer)
    Select Case Index
        Case 0:
            dtpDay(0).Value = Format(DateAdd("m", -1, Date), "YYYY-MM-DD")
            dtpDay(1).Value = Format(Date, "YYYY-MM-DD")
        
        Case 1:
            dtpDay(0).Value = Format(DateAdd("ww", -1, Date), "YYYY-MM-DD")
            dtpDay(1).Value = Format(Date, "YYYY-MM-DD")
        
        Case 2:
            dtpDay(0).Value = Format(Date, "YYYY-MM-DD")
            dtpDay(1).Value = Format(Date, "YYYY-MM-DD")
    End Select
    
    Call Data_Display
End Sub

Private Sub chkTotal_Click(Value As Integer)
    dtpDay(0).Enabled = Not dtpDay(0).Enabled
    dtpDay(1).Enabled = Not dtpDay(1).Enabled
End Sub

Private Sub cmdBtn_Click(Index As Integer)
    Select Case Index
        Case 3:
            Call Export_Excel(frmMain.cdgExcel, sprGrid)
            'Call sprGrid.ExportExcelBook("test.xls", "")
            'Call sprGrid.ExportToTextFile("test.csv", """", ",", vbCrLf, ExportToTextFileCreateNewFile, "")
            
        Case 4:
            Rtn = MsgBox("출력 미리보기를 하시겠습니까?", vbQuestion + vbYesNo, "출력")
            
            If Rtn = vbYes Then
                Call Data_Print(True)
            Else
                Call Data_Print(False)
            End If
            
        Case 5: Unload Me
    End Select
End Sub

Private Sub Data_Print(Print_PreView As Boolean)
    On Error GoTo ErrRtn
    
    If sprGrid.MaxRows = 0 Then Exit Sub

    If Dir(AppPath & "XML", vbDirectory) = "" Then
        MkDir AppPath & "XML"
    End If

    Open AppPath & "XML\접수현황.XML" For Output As #1
    
    Print #1, "<?xml version=""1.0"" encoding=""EUC-KR""?>"
    Print #1, "<root>"
        
          XML = "    <조건>"
          
    If chkTotal.Value = -1 Then
        XML = XML & "        <접수일자>접수일자 : 전체기간</접수일자>"
    Else
        XML = XML & "        <접수일자>접수일자 : " & Format(dtpDay(0).Value, "YYYY-MM-DD") & " ~ " & Format(dtpDay(1).Value, "YYYY-MM-DD") & "</접수일자>"
    End If
    
    XML = XML & "        <가맹점>크린에이드 - " & Func_Replace(가맹점정보.가맹점명) & "</가맹점>"
    XML = XML & "   </조건>"
    Print #1, XML
    
    With sprGrid
        For i = 1 To .MaxRows
            .Row = i
            
                             XML = "    <Data>"
            .Col = 5:  XML = XML & "        <접수일자>" & Format(.Text, "YYYY-MM-DD") & "</접수일자>"
            .Col = 2:  XML = XML & "        <성명>" & Func_Replace(.Text) & "</성명>"
            .Col = 3:  XML = XML & "        <휴대전화>" & Func_Replace(.Text) & "</휴대전화>"
            .Col = 4:  XML = XML & "        <전화번호>" & Func_Replace(.Text) & "</전화번호>"
            .Col = 8:  XML = XML & "        <출고일자>" & Format(.Text, "YYYY-MM-DD") & "</출고일자>"
            .Col = 14: XML = XML & "        <지사출고일>" & Format(.Text, "YYYY-MM-DD") & "</지사출고일>"
            .Col = 15: XML = XML & "        <택번호>" & .Text & "</택번호>"
            .Col = 16: XML = XML & "        <품명>" & Func_Replace(.Text) & "</품명>"
            .Col = 17: XML = XML & "        <색상>" & Func_Replace(.Text) & "</색상>"
            .Col = 18: XML = XML & "        <무늬>" & Func_Replace(.Text) & "</무늬>"
            .Col = 19: XML = XML & "        <내용>" & Func_Replace(.Text) & "</내용>"
            .Col = 20: XML = XML & "        <금액>" & .Text & "</금액>"
            .Col = 21: XML = XML & "        <결제>" & Func_Replace(.Text) & "</결제>"
            .Col = 22: XML = XML & "        <상표>" & Func_Replace(.Text) & "</상표>"
                       XML = XML & "   </Data>"
                       Print #1, XML
        Next i
        
        Print #1, "</root>"
        Close #1
    End With
    
    If Print_PreView = True Then
        With rpt접수현황
            .dc.FileURL = AppPath & "XML\접수현황.XML"
            .Show 1
        End With
    Else
        With rpt접수현황
            .dc.FileURL = AppPath & "XML\접수현황.XML"
            .PrintReport False
        End With
    
        Unload rpt접수현황
    End If
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.description)

    Screen.MousePointer = 0
End Sub

Private Sub Data_Display()
    On Error GoTo ErrRtn
    
    If (chkTotal.Value = -1) And (Trim(txtFind.Text) = "") Then
        MsgBox "검색조건을 입력하세요.", vbInformation, "확인"
        
        txtFind.SetFocus
        Exit Sub
    End If
    
    pnlProg.Visible = True
    DoEvents
            
    Query = "SELECT    A.접수번호"
    Query = Query & ", A.접수일자"
    Query = Query & ", A.접수시간"
    Query = Query & ", A.예정일자"
    Query = Query & ", A.출고일자"
    Query = Query & ", A.출고시간"
    Query = Query & ", A.지사출고상태"
    Query = Query & ", A.의류명"
    Query = Query & ", A.택번호"
    Query = Query & ", A.색상"
    Query = Query & ", A.무늬"
    Query = Query & ", A.내용"
    Query = Query & ", A.금액"
    Query = Query & ", A.결제여부"
    Query = Query & ", A.상표"
    Query = Query & ", A.오점내용"
    Query = Query & ", SUBSTRING(A.가맹점출고일자,1,10) AS 가맹점출고일자"
    Query = Query & ", SUBSTRING(A.가맹점입고일자,1,10) AS 가맹점입고일자"
    Query = Query & ", A.가맹점입고구분"
    Query = Query & ", SUBSTRING(A.지사입고일자,1,10)   AS 지사입고일자"
    Query = Query & ", SUBSTRING(A.지사출고일자,1,10)   AS 지사출고일자"
    Query = Query & ", B.성명"
    Query = Query & ", B.휴대전화"
    Query = Query & ", B.전화번호"
    Query = Query & " FROM TB_입출고 AS A LEFT OUTER JOIN TB_고객정보 AS B ON (A.고객코드 = B.고객코드) "
    
    If chkTotal.Value = -1 Then
        Query = Query & " WHERE (A.접수일자 >= '0000-00-00')"
    Else
        Query = Query & " WHERE (A.접수일자 >= '" & Format(dtpDay(0).Value, "YYYY-MM-DD") & "'"
        Query = Query & "   AND  A.접수일자 <= '" & Format(dtpDay(1).Value, "YYYY-MM-DD") & "')"
    End If
    
    Query = Query & "   AND (A.판매취소 <> 'Y')"

'    Query = Query & "   AND ((A.판매취소 <> 'Y')"
'    Query = Query & "   AND (A.반품환불일자 IS NULL OR A.반품환불일자 = '')"
'    Query = Query & "   AND (A.세탁환불일자 IS NULL OR A.세탁환불일자 = ''))"
    
    If txtTAGNo.RawData <> "" Then
        Query = Query & " AND SUBSTRING(A.택번호,4,6) = '" & txtTAGNo.RawData & "'"
    End If
    
    If txtFind.Text <> "" Then
        Select Case cboGubun.Text
            Case "성명":     Query = Query & " AND (B.성명 LIKE '%" & Trim(txtFind.Text) & "%') "
            Case "전화번호": Query = Query & " AND (B.전화번호 LIKE '%" & Trim(txtFind.Text) & "%') "
            Case "고객코드": Query = Query & " AND (B.고객코드 LIKE '%" & Trim(txtFind.Text) & "%') "
        End Select
    End If
    
    'Query = Query & " ORDER BY A.접수일자 DESC, A.접수번호 DESC, A.택번호 DESC"
    Set ADORs = New ADODB.Recordset
    ADORs.Open Query, ADOCon, adOpenForwardOnly, adLockReadOnly

    With sprGrid
        .MaxRows = 0
        .ReDraw = False
        
        Do Until ADORs.EOF
            
            
            .MaxRows = .MaxRows + 1
            .Row = .MaxRows
            
            .Col = 1:  .Text = ADORs!접수번호 & ""                                ' 1
            
            If Trim(ADORs!성명) = "" Then
                .Col = 2: .Text = " "                                             ' 2
            Else
                .Col = 2: .Text = ADORs!성명 & ""                                 ' 2
            End If
            
            If Trim(ADORs!휴대전화) = "" Then
                .Col = 3: .Text = " "                                             ' 3
            Else
                .Col = 3: .Text = ADORs!휴대전화 & ""                             ' 3
            End If
            
            If Trim(ADORs!전화번호) = "" Then
                .Col = 4: .Text = " "                                             ' 4
            Else
                .Col = 4: .Text = ADORs!전화번호 & ""                             ' 4
            End If
            
            .Col = 5:  .Text = Format(ADORs!접수일자, "YY-MM-DD") & ""            ' 5
            
            If ADORs!접수시간 = "" Then
                .Col = 6: .Text = " "                                             ' 6
            Else
                .Col = 6: .Text = Left(ADORs!접수시간, 5) & ""                    ' 6
            End If
            
            .Col = 7:  .Text = Format(ADORs!예정일자, "YY-MM-DD") & ""            ' 7
            .Col = 8:
                If ADORs!출고일자 <> "" Then
                    .Text = Format(ADORs!출고일자, "YY-MM-DD") & ""
                Else
                    .Text = ""
                End If          ' 8
            .Col = 9:  .Text = Left(ADORs!출고시간, 5) & ""                       ' 9
            .Col = 10: .Text = Format(ADORs!가맹점출고일자, "YY-MM-DD") & ""      '10
            
            '------------------------------------------------------------------------
            If Trim(ADORs!가맹점입고일자) = "00:00:00" Then
                .Col = 11: .Text = Format(ADORs!지사출고일자, "YY-MM-DD") & ""    '11
            Else
                .Col = 11: .Text = Format(ADORs!가맹점입고일자, "YY-MM-DD") & ""  '11
            End If
            
                           If Trim(.Text) = "" Then .BackColor = vbYellow         '11
            '------------------------------------------------------------------------
            .Col = 12: .Text = ADORs!가맹점입고구분 & ""       '12
            
            .Col = 13: .Text = Format(ADORs!지사입고일자, "YY-MM-DD") & ""        '12
            
            '------------------------------------------------------------------------
            .Col = 14: .Text = Format(ADORs!지사출고일자, "YY-MM-DD") & ""        '13
            
                       If Trim(.Text) = "" Then .BackColor = vbYellow             '13
            '------------------------------------------------------------------------
            'If (ADORs!택번호 = "122586588") Then Debug.Assert (False)
            If Len(ADORs!택번호) = 9 Then
                .Col = 15: .Text = Format(Right(ADORs!택번호, 6), "00-0000")      '14
            Else
                .Col = 15: .Text = ADORs!택번호 & ""                              '14
            End If
            
            .Col = 16: .Text = ADORs!의류명 & ""                                  '15
            .Col = 17: .Text = ADORs!색상 & ""                                    '16
            .Col = 18: .Text = ADORs!무늬 & ""                                    '17
            .Col = 19: .Text = ADORs!내용 & ""                                    '18
            .Col = 20: .Text = ADORs!금액 & ""                                    '19
            .Col = 21: .Text = ADORs!결제여부 & ""                                '20
            .Col = 22: .Text = ADORs!상표 & ""                                    '21
            .Col = 23: .Text = ADORs!오점내용 & ""                                '22
            
            ADORs.MoveNext
        Loop
        ADORs.Close
        Set ADORs = Nothing
        
        .SortKey(1) = 5                                                           '접수일자
        .SortKeyOrder(1) = SortKeyOrderDescending                                 'DESC

        .SortKey(2) = 1                                                           '접수번호
        .SortKeyOrder(2) = SortKeyOrderDescending                                 'DESC

        .SortKey(3) = 14                                                          '택번호
        .SortKeyOrder(3) = SortKeyOrderDescending                                 'DESC

        .Sort -1, -1, -1, -1, SortByRow
        
        .ReDraw = True
    End With
    
    Call Total_Display
    
    pnlProg.Visible = False
    
    Exit Sub
    
ErrRtn:
    pnlProg.Visible = False
    
    Call Error_Msg("", Err.Source, Err.Number, Err.description)
End Sub

Private Sub cmdFind1_Click()
    On Error GoTo ErrRtn
    
    If cboGubun.Text = "의류명" Then Exit Sub
    
    '-----------------------------------------------------------------------------------
    ' TB_고객
    '-----------------------------------------------------------------------------------
    Query = "SELECT    고객코드"
    Query = Query & ", 성명"
    Query = Query & ", 전화번호"
    Query = Query & ", 주소"
    Query = Query & " FROM TB_고객정보"
    Query = Query & " WHERE 삭제 = 0" '
    
    If txtFind.Text <> "" Then
        If cboGubun.Text = "전화번호" Then
            Query = Query & " AND (전화번호  LIKE '%" & txtFind.Text & "%'"
            Query = Query & "  OR  휴대전화 LIKE '%" & txtFind.Text & "%')"
            Query = Query & " ORDER BY " & cboGubun.Text & " ASC"
        Else
            Query = Query & " AND " & cboGubun.Text & " LIKE '%" & txtFind.Text & "%' "
            Query = Query & " ORDER BY " & cboGubun.Text & " ASC"
        End If
    Else
        Query = Query & " ORDER BY 성명 ASC"
    End If
    
    'If txtFind.Text <> "" Then
    '    Query = Query & " AND 성명 LIKE '%" & txtFind.Text & "%' "
    'End If
    
    'Query = Query & " ORDER BY 성명 ASC "
    Set ADORs = New ADODB.Recordset
    ADORs.Open Query, ADOCon, adOpenStatic, adLockReadOnly
    
    With fpList1
        Set .DataSource = ADORs
        
        .Top = 1530
        .Left = 2445
        
        .Visible = True
        .SetFocus
    End With
    
    ADORs.Close
    Set ADORs = Nothing
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.description)

    Screen.MousePointer = 0
End Sub

Private Sub fpList1_DblClick()
    On Error GoTo ErrRtn
    
    With fpList1
        .Col = 0: .Row = .ListIndex: txtFind.Tag = Trim(.ColList)  '코드
        .Col = 1: .Row = .ListIndex: txtFind.Text = Trim(.ColList) '이름
        
        If txtFind.Tag = "" Then
            .Visible = False
            
            txtFind.SetFocus
            Exit Sub
        End If
        
        .Visible = False
        
        Call cmdList_Click
    End With
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.description)

    Screen.MousePointer = 0
End Sub

