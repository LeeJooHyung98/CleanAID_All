    Query = Query & "   AND 접수금액 = 0" ' 판매취소시 0원으로 들어온다.
    Query = Query & "   AND 적요  LIKE '%미수금액 입금%'"

    Set ADORs = New ADODB.Recordset
    ADORs.Open Query, ADOCon, adOpenForwardOnly, adLockReadOnly

    txtNum(13).Value = ADORs(1)

    ADORs.Close:    Set ADORs = Nothing
    txtNum(14).Value = txtNum(12).Value + txtNum(13).Value      '미수금입금 = 현금 + 카드
    
    
    'txtNum(3).Value = txtNum(5).Value + txtNum(4).Value                     '총매출 = 순매출 + 반품금액
    '수금액 = 현금 + 카드+쿠폰+마일리지+미수금
    txtNum(11).Value = txtNum(6).Value + txtNum(7).Value + txtNum(8).Value + txtNum(9).Value + txtNum(10).Value
    
'    txtNum(10).Value = txtNum(5).Value - txtNum(11).Value - txtNum(9).Value  '미수금액
    
    txtNum(17).Value = txtNum(6).Value + txtNum(12).Value       '현금
    txtNum(18).Value = txtNum(7).Value + txtNum(13).Value       '카드
    txtNum(19).Value = txtNum(8).Value                          '쿠폰
    txtNum(15).Value = txtNum(9).Value                          '마일리지
    txtNum(16).Value = txtNum(10).Value                         '미수
    txtNum(20).Value = txtNum(17).Value + txtNum(18).Value + txtNum(19).Value + txtNum(15).Value + txtNum(16).Value
    
    
    Exit Sub
    
ErrRtn:
    Call Error_Msg("", Err.Source, Err.Number, Err.description)

    Screen.MousePointer = 0
